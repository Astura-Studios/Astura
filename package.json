{
    "name": "astura",
    "version": "1.0.0",
    "description": "The official Discord bot for the Astura Studios Discord community server.",
    "main": "dist/Index.js",
    "scripts": {
        "dev": "tsc && node --max-old-space-size=4096 dist/Index.js",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "prettier": "prettier dist",
        "prettier:fix": "prettier --write dist",
        "push": "node push.js",
        "test": "node dist/Index.js"
    },
    "repository": {
        "type": "git",
        "url": "+https://github.com/Astura-Studios/Astura.git"
    },
    "keywords": [
        "Discord",
        "Bot",
        "discord.js",
        "Astura",
        "NodeJS"
    ],
    "author": "Ascendus",
    "bugs": {
        "url": "https://github.com/Astura-Studios/Astura/issues"
    },
    "contributors": [
        "Ascendus",
        "lucaslah"
    ],
    "license": "ISC",
    "dependencies": {
        "@discordjs/builders": "^0.6.0",
        "@discordjs/collection": "^0.2.1",
        "@discordjs/rest": "^0.1.0-canary.0",
        "@mikro-orm/cli": "^4.5.9",
        "@mikro-orm/core": "^4.5.9",
        "@mikro-orm/reflection": "^4.5.9",
        "@mikro-orm/sqlite": "^4.5.9",
        "canvas": "^2.8.0",
        "common-tags": "^1.8.0",
        "discord-api-types": "^0.24.0",
        "discord.js": "^13.4.0-dev.1635552294.761a901",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "figlet": "^1.5.2",
        "node-fetch": "^2.6.5",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4",
        "url": "^0.11.0"
    },
    "devDependencies": {
        "@types/common-tags": "^1.8.1",
        "@types/continuation-local-storage": "^3.2.3",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/figlet": "^1.5.4",
        "@types/node": "^16.11.6",
        "@types/node-fetch": "^2.5.12",
        "@typescript-eslint/eslint-plugin": "5.2.0",
        "@typescript-eslint/parser": "5.2.0",
        "eslint": "7.32.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "husky": "7.0.4",
        "lint-staged": "11.2.6",
        "prettier": "^2.4.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.ts": [
            "eslint --fix",
            "git add"
        ]
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./src/structures/database/mikro-orm.config.ts",
            "./dist/structures/database/mikro-orm.config.js"
        ]
    }
}
