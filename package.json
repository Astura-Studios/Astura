{
    "name": "astura",
    "version": "1.0.0",
    "description": "The official Discord bot for the Astura Studios Discord community server.",
    "main": "build/index.js",
    "types": "./typings/**/*",
    "scripts": {
        "commit": "node build/commit.js",
        "db:migrate": "npx prisma migrate dev --name init",
        "db:push": "npx prisma db push",
        "db:pull": "npx prisma db pull",
        "dev": "SET NODE_ENV=development && tsc && nodemon",
        "start": "SET NODE_ENV=production && tsc && node build/index.js",
        "guide": "start https://discordjs.guide/#before-you-begin",
        "docs": "start https://discord.js.org/#/docs/stable",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
        "lint:fix": "eslint --fix",
        "test": "echo Error: no test specified && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "+https://github.com/Astura-Studios/Astura.git"
    },
    "keywords": [
        "Discord",
        "Bot",
        "discord.js",
        "Astura",
        "NodeJS"
    ],
    "author": {
        "name": "Astura Studios",
        "email": "astura.studios@gmail.com",
        "url": "https://github.com/Astura-Studios"
    },
    "bugs": {
        "url": "https://github.com/Astura-Studios/Astura/issues"
    },
    "contributors": [
        "Ascendus",
        "lucaslah"
    ],
    "license": "ISC",
    "dependencies": {
        "@discordjs/builders": "^0.13.0",
        "@discordjs/collection": "^0.6.0",
        "@discordjs/rest": "^0.4.1",
        "@prisma/client": "^3.14.0",
        "chalk": "4.1.2",
        "chalk-table": "^1.0.2",
        "common-tags": "^1.8.2",
        "discord-api-types": "^0.32.1",
        "discord.js": "^13.7.0",
        "nodemon": "    ^2.0.15",
        "prisma": "^3.10.0",
        "ts-node": "^10.4.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/common-tags": "^1.8.1",
        "@types/node": "^16.10.3",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.1",
        "typescript": "^4.4.3"
    }
}
