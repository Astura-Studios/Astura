{
    "name": "astura",
    "version": "1.0.0",
    "description": "The official Discord bot for the Astura Studios Discord community server.",
    "main": "build/index.js",
    "types": "./typings/**/*",
    "scripts": {
        "commit": "node build/commit.js",
        "dev": "SET NODE_ENV=development && tsc && nodemon",
        "start": "SET NODE_ENV=production && tsc && node build/index.js",
        "guide": "start https://discordjs.guide/#before-you-begin",
        "docs": "start https://discord.js.org/#/docs/stable",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
        "lint:fix": "eslint --fix",
        "schema:migrate": "npx prisma migrate dev --name init",
        "schema:push": "npx prisma db push",
        "test": "echo Error: no test specified && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "+https://github.com/Astura-Studios/Astura.git"
    },
    "keywords": [
        "Discord",
        "Bot",
        "discord.js",
        "Astura",
        "NodeJS"
    ],
    "author": {
        "name": "Astura Studios",
        "email": "astura.studios@gmail.com",
        "url": "https://github.com/Astura-Studios"
    },
    "bugs": {
        "url": "https://github.com/Astura-Studios/Astura/issues"
    },
    "contributors": [
        "Ascendus",
        "lucaslah"
    ],
    "license": "ISC",
    "dependencies": {
        "@discordjs/builders": "^0.9.0",
        "@discordjs/collection": "^0.4.0",
        "@discordjs/rest": "^0.3.0",
        "@prisma/client": "^3.10.0",
        "ansi-regex": "^6.0.1",
        "canvacord": "^5.2.3",
        "canvas": "^2.9.0",
        "chalk": "4.1.2",
        "chalk-table": "^1.0.2",
        "common-tags": "^1.8.2",
        "cors": "^2.8.5",
        "discord-api-types": "^0.26.1",
        "discord.js": "^13.2.0",
        "encoding": "^0.1.13",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "http-server": "^13.0.2",
        "nodemon": "^2.0.15",
        "prisma": "^3.10.0",
        "simple-get": "4.0.1",
        "sqlite": "^4.0.23",
        "sqlite3": "^4.2.0",
        "ts-node": "^10.4.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/common-tags": "^1.8.1",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/http-server": "^0.12.1",
        "@types/node": "^16.10.3",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.1",
        "typescript": "^4.4.3"
    }
}